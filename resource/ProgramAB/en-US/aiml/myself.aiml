  <category><pattern>AGE</pattern>
    <template><think>
        <set var="years">
        <interval>
        <jformat>dd MM yyyy</jformat>
        <style>years</style>
        <from><bot name="birth"/></from>
        <to><date jformat="dd MM yyyy"/></to>
      </interval>
        </set>
        <set var="months">
        <interval>
        <jformat>dd MM yyyy</jformat>
        <style>months</style>
        <from><bot name="birth"/></from>
        <to><date jformat="dd MM yyyy"/></to>
      </interval>
        </set>
      </think>
      <condition var="years">
        <li value="0">I am <get var="months"/> months old.</li>
        <li>I am <get var="years"/> years old.</li>
      </condition></template>
