        <srai><star index="2" /> <star index="3" /> <star index="4" /></srai></template>
  </category>
  <category>
    <pattern>DO YOU KNOW *</pattern>
    <template>Yes, 
        <srai><star /></srai></template>
  </category>
  <category>
    <pattern>YOU KNOW *</pattern>
    <template>Yes, 
        <srai><star /></srai></template>
  </category>
  <category>
    <pattern>YOU KNOW * *</pattern>
    <template>Yes, 
        <srai><star /> <star index="2" /></srai></template>
  </category>
  <category>
    <pattern>YOU KNOW * * *</pattern>
    <template>Yes, 
        <srai><star /> <star index="2" /> <star index="3" /></srai></template>
  </category>
  <category>
    <pattern><set>determinantinterogatif</set> YOU KNOW *</pattern>
    <template>Yes, 
        <srai><star /></srai></template>
  </category>
  <category>
    <pattern><set>determinantinterogatif</set> YOU KNOW * *</pattern>
