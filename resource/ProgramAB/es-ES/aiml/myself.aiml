  <category><pattern>EDAD</pattern>
    <template><think>
        <set var="years">
        <interval>
        <jformat>dd MM yyyy</jformat>
        <style>years</style>
        <from><bot name="birth"/></from>
        <to><date jformat="dd MM yyyy"/></to>
      </interval>
        </set>
        <set var="months">
        <interval>
        <jformat>dd MM yyyy</jformat>
        <style>months</style>
        <from><bot name="birth"/></from>
        <to><date jformat="dd MM yyyy"/></to>
      </interval>
        </set>
      </think>
      <condition var="years">
        <li value="0">Tengo <get var="months"/> meses.</li>
        <li>Tengo <get var="years"/> años.</li>
      </condition></template>
